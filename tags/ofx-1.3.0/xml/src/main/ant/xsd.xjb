    <jxb:bindings schemaLocation="../../../../../../exlp/xml/src/main/xsd/util/io.xsd" node="/xsd:schema">
        <jxb:schemaBindings>
            <jxb:package name="net.sf.exlp.xml.io"/>
        </jxb:schemaBindings>
