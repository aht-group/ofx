<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<metatag xmlns="http://www.openfuxml.org/jsf" xmlns:ofx="http://www.openfuxml.org">
	<tag>
		<description>A iframe</description>
		<name>iframe</name>
		<tag-class>org.openfuxml.addon.jsf.tld.html.tag.IframeTag</tag-class>
		<body-content>JSP</body-content>
		<attribute>
			<description>@@@</description>
			<name>id</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>@@@</description>
			<name>binding</name>
			<deferred-value>javax.faces.component.UIComponent</deferred-value>
		</attribute>
		<attribute>
			<description>@@@</description>
			<name>rendered</name>
			<deferred-value><type>boolean</type></deferred-value>
		</attribute>
		<attribute>
			<description>src</description>
			<name>src</name>
			<required>true</required>
			<deferred-value><type>java.lang.String</type></deferred-value>
		</attribute>
		<attribute>
			<description>width</description>
			<name>width</name>
			<required>true</required>
			<deferred-value><type>java.lang.String</type></deferred-value>
		</attribute>
		<attribute>
			<description>height</description>
			<name>height</name>
			<required>true</required>
			<deferred-value><type>java.lang.String</type></deferred-value>
		</attribute>
		<attribute>
			<description>style</description>
			<name>style</name>
			<required>false</required>
			<deferred-value><type>java.lang.String</type></deferred-value>
		</attribute>
	</tag>
	<component>
		<component-type>org.openfuxml.jsf.Iframe</component-type>
		<component-class>org.openfuxml.addon.jsf.tld.html.ui.UIIframe</component-class>
	</component>
	<renderer>
		<component-family>javax.faces.Output</component-family>
		<renderer-type>org.openfuxml.jsf.Iframe</renderer-type>
		<renderer-class>org.openfuxml.addon.jsf.tld.html.renderer.IframeRenderer</renderer-class>
	</renderer>
</metatag>